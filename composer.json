{
    "name": "ondrejmirtes/mocktainer",
    "type": "library",
    "license": "MIT",
	"description": "Create objects of classes with mocked dependencies more easily!",
    "authors": [
        {
            "name": "Ondrej Mirtes",
            "email": "ondrej@mirtes.cz"
        }
    ],
    "require": {
		"php": "^5.6|^7.0",
		"phpunit/phpunit": "~5.0"
	},
	"require-dev": {
		"consistence/coding-standard": "~0.8.0",
		"jakub-onderka/php-parallel-lint": "~0.8",
		"satooshi/php-coveralls": "dev-master",
		"squizlabs/php_codesniffer": "~2.3"
	},
	"autoload": {
		"psr-4": {
			"Mocktainer\\": [ "src" ]
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests"
		]
	},
	"config": {
		"bin-dir": "bin"
	}
}
