{
    "name": "ondrejmirtes/mocktainer",
    "type": "library",
    "license": "MIT",
	"description": "Create objects of classes with mocked dependencies more easily!",
    "authors": [
        {
            "name": "Ondrej Mirtes",
            "email": "ondrej@mirtes.cz"
        }
    ],
    "require": {
		"php": "^7.2",
		"phpunit/phpunit": "^8.0"
	},
	"require-dev": {
		"consistence/coding-standard": "~0.13.0",
		"jakub-onderka/php-parallel-lint": "^0.9",
		"phpstan/phpstan": "^0.9",
		"php-coveralls/php-coveralls": "2.1.0",
		"slevomat/coding-standard": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"Mocktainer\\": [ "src" ]
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests"
		]
	},
	"config": {
		"bin-dir": "bin"
	}
}
